<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.babyneedswithbita">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BabyNeedsWithBita">
        <activity android:name=".BabyNeedsList"></activity>
        <activity
            android:name=".BabyNeedsStartPage"
            android:label="@string/title_activity_baby_needs_start_page"
